import java.util.ArrayList;
import java.util.Arrays;

/*
 *  프로그래머스 문제 풀이
 *  https://school.programmers.co.kr/learn/courses/30/lessons/42576
 *  입력 : 마라톤에 참여한 선수들의 이름이 담긴 배열 participant와 완주한 선수들의 이름이 담긴 배열 completion
 *  출력 : 완주하지 못한 선수의 이름
 *  기능 : 마라톤 선수들 중 완주하지 못한 선수를 찾는 함
 */

public class June_01_02 {
	public String solution(String[] participant, String[] completion) {
		if((completion == null) || (participant == null)) {
			return "";
		}
		ArrayList<String> participantList = new ArrayList<>(Arrays.asList(participant));
        for(String person : completion) {
        	boolean isRemoved = participantList.remove(person);
        	if(!isRemoved) {
        		return "";
        	}
        }
        if(participant == null) {
        	return "";
        }
        else if(participant.length == 1) {
        	return participantList.get(0);
        }
        else {
        	return "";
        }
    }
}
